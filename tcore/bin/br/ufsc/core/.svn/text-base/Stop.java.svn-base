package br.ufsc.core;

import java.sql.Timestamp;

/**
 * 
 * @author André Salvaro Furtado
 *
 */
public class Stop {
	
	private int tid;
	private int stopId;
	private String stopName;
	private Timestamp startTime;
	private Timestamp endTime;
	private double avg;
	// (spatial) relevant feature
	private int rfid;
	private String rfTableName;
	private String geom;
	// points interval - indexes
	private int begin, end;
	
	
	public Stop(int tid, int stopId, String stopName, Timestamp startTime, Timestamp endTime, double avg, int rfId, String rfTableName, int begin, int end, String geom) {
		this.tid = tid;
		this.stopId = stopId;
		this.stopName = stopName;
		this.startTime = startTime;
		this.endTime = endTime;
		this.avg = avg;
		this.rfid = rfId;
		this.rfTableName = rfTableName;
		this.begin = begin;
		this.end = end;
		this.geom = geom;
	}


	public int getTid() {
		return tid;
	}

	public int getStopId() {
		return stopId;
	}

	public String getStopName() {
		return stopName;
	}

	public Timestamp getStartTime() {
		return startTime;
	}

	public Timestamp getEndTime() {
		return endTime;
	}

	public double getAvg() {
		return avg;
	}

	public int getRfid() {
		return rfid;
	}

	public String getRfTableName() {
		return rfTableName;
	}

	public int getBegin() {
		return begin;
	}

	public int getEnd() {
		return end;
	}
	
	public String getGeom(){
		return geom;
	}
	
}
