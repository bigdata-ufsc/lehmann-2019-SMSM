{
	"dataset": {
		"name" : "geolife2",
		"raw": 'false',
		"params": {
			"pointsTable" : "public.geolife2_cleared",
			"stopTable" : "public.geolife2_stop",
			"moveTable" : "public.geolife2_move",
			"mappingTable" : "public.geolife2_stops_moves"
		}
	},
	"groundtruth": {
		"type": "String",
		"index": 10
	},
	"measure": {
		"name": "MSTP",
		"params": [
		{
			"type": "Semantic",
			"index" : 6,
			"params" : [{
				"distance": "equal",
				"type" : "Region"
			}]
		}
		, 
		{
			"type": "temporal",
			"distance": "proportion",
			"threshold": "0.1",
			"params" : [{
				"threshold": 2
			}],
			"index" : 2
		},
		{
			"type": "spatial",
			"distance": "euclidean",
			"threshold": "200",
			"index" : 1
		}
		]
	}
}