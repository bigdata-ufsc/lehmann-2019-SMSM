{
	"dataset": {
		"name" : "geolife2",
		"raw": 'false',
		"min-trajectories-per-class": 4,
		"params": {
			"pointsTable" : "geolife.geolife_inside_university_5_pois_enhanced",
			"stopTable" : "stops_moves.geolife_inside_enhanced_stop",
			"moveTable" : "stops_moves.geolife_inside_enhanced_move"
		}
	},
	"groundtruth": {
		"type": "String",
		"index": 10
	},
	"measure": {
