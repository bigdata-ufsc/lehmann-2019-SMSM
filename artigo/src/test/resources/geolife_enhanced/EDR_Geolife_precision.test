			"index" : 8,
			"params" : [{
				"distance": "equal",
				"type" : "Region"
			}]
		}
		, 
		/*{
			"type": "temporal",
			"distance": "slack-proportion",
			"threshold": "0.1",
			"params" : [{
				"threshold": 2
			}],
			"index" : 2
		},*/
		{
			"type": "spatial",
			"distance": "euclidean",
			"threshold": "200",
			"index" : 1
		}
		]
	}
}