{
	"dataset": {
		"name" : "ais-brest",
		"raw": 'true'
	},
	"groundtruth": {
		"type": "Number",
		"index": 3
	},
	"measure": {
		"name": "CATS",
		"params": [{
			"type": "spatial",
			"index" : 1,
			"distance": "euclidean",
			"threshold": "[0, 50, 100, 200, 500]"
		}, {
			"type": "temporal",
			"index" : 2,
			"distance": "timestamp",
			"threshold": "[0,5,10,15,25]",
			"params": [{
				"type": "minutes"
			}]
		}]
	}
}