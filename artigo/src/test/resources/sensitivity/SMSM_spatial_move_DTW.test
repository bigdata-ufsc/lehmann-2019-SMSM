{
	"dataset": {
		"name" : "geolife_university",
		"raw": 'false',
		"params": {
			"pointsTable" : "geolife.geolife_inside_university_5_pois",
			"stopTable" : "stops_moves.geolife_inside_university_stop_5_pois",
			"moveTable" : "stops_moves.geolife_inside_university_move_5_pois"
		}
	},
	"groundtruth": {
		"type": "String",
		"index": 10
	},
	"measure": {
		"name": "SMSM",
		"params": [{
			"type": "Stop",
			"weight": 0.5,
			"index" : 8,
			"params": [{
				"type": "spatial",
				"distance": "euclidean",
				"threshold": "[20,40,60,80,100,200,300,400,500,600,700,800,900,1000]",
				"index" : 1,
				"weight": 0.5
			}, {
				"type": "Semantic",
				"index" : 8,
				"weight": 0.5,
				"params" : [{
					"distance": "Equal",
					"type" : "REGION"
				}]
			}]
		}, {
			"type": "Move",
			"weight": 0.5,
			"index" : 9,
			"params": [{
				"type": "POINTS",
				"distance": "DTW",
				"threshold": "[summed-distances*0.25, summed-distances*0.50, summed-distances*0.75, summed-distances, summed-distances*1.5, summed-distances*2, summed-distances*3, summed-distances*5, summed-distances*7.5, summed-distances*10]",
				"weight": 1,
				"params" : [{
					"distance" : "euclidean"
				}]
			}]
		}]
	}
}